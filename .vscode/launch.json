{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Django: Runserver",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/manage.py",
        "args": [
          "runserver",
          "127.0.0.1:8000"
        ],
        "django": true,
        "env": {
          "pythonPath": "${workspaceFolder}/venv/bin/python",
          "DJANGO_SETTINGS_MODULE": "stock_backend.settings"
        },
        "justMyCode": true
      }
    ]
  }
  